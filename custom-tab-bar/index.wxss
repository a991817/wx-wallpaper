.custom-tab-bar-wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.custom-tab-bar-wrapper .text {
  font-size: 20rpx;
}

.demo__list {
  display: flex;
  flex-wrap: wrap;
  padding: 16rpx 32rpx;
}

.demo__card {
  flex: 0 0 25%;
  text-align: center;
  margin-bottom: 30rpx;
}

.demo__card-name {
  font-size: 24rpx;
  color: #999;
}